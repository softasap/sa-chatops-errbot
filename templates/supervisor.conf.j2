[program:errbot]
command = {{ errbot_venv_dir }}/bin/errbot --config {{ errbot_install_dir }}/config.py
user = {{ errbot_user }}
stdout_logfile = /var/log/supervisor/errbot.log
stderr_logfile = NONE
redirect_stderr = true
directory = {{ errbot_install_dir }}
startsecs = 3
stopsignal = INT
environment = LC_ALL="en_US.UTF-8"